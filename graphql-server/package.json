{
  "name": "setlist-generator-graphql-server",
  "version": "1.0.0",
  "description": "GraphQL server for setlist generator",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "apollo-server-express": "^3.12.0",
    "@prisma/client": "^6.2.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "graphql": "^15.8.0",
    "jsonwebtoken": "^9.0.2",
    "type-graphql": "^1.1.1",
    "reflect-metadata": "^0.2.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^22.5.0",
    "prisma": "^6.2.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.6.0"
  },
  "engines": {
    "node": "20.11.1"
  },
  "pnpm": {
    "overrides": {
      "graphql": "^15.8.0"
    }
  }
}